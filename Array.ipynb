   "source": [
    "# Problem: Two Sum\n",
    "# Approach: HashMap\n",
    "# Time: O(n)\n",
    "# Space: O(n)\n",
    "\n",
    "class Solution:\n",
    "    def twoSum(self, nums, target):\n",
    "        seen = {}\n",
    "        for i, num in enumerate(nums):\n",
    "            if target - num in seen:\n",
    "                return [seen[target - num], i]\n",
    "            seen[num] = i\n"
   ]
  }
